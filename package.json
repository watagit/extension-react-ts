{
  "name": "browser-extension-react-ts-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.2.0",
  "scripts": {
    "dev": "run-s clean && vite",
    "build": "run-s clean && tsc && vite build && run-s firefox-mv2-build",
    "firefox-mv2-build": "node tools/unstable_mv3ToMv2Build.js",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:watch": "tsc --noEmit -w",
    "clean:dist-firefox-v2": "rimraf dist-firefox-v2",
    "check": "biome check --write src/**"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "framer-motion": "^11.2.10",
    "lodash-es": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@crxjs/vite-plugin": "^2.0.0-beta.17",
    "@types/chrome": "^0.0.268",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.8",
    "construct-style-sheets-polyfill": "^3.1.0",
    "fs-extra": "^11.1.1",
    "identity-obj-proxy": "^3.0.0",
    "npm-run-all2": "^6.0.5",
    "postcss": "^8.4.24",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.3.2",
    "typescript": "4.9.5",
    "vite": "^4.3.9"
  }
}
